    "mongodb": "^4.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.15.0",
    "nodemon": "^2.0.16"
  }
